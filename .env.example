# Environment Variables for Surgical Tool Tracking Research
# =========================================================

# CholecTrack20 Dataset Access Key (obtained from Synapse.org)
CHOLECTRACK20_DATASET_ACCESS_KEY=
SYNAPSE_AUTH_TOKEN= SYNAPSE_AUTH_TOKEN
SYNAPSE_EMAIL=

# Weights & Biases Configuration
WANDB_PROJECT=surgical-tool-tracking
WANDB_ENTITY=your_wandb_username

# Dataset Configuration
DATASET_ROOT=./data/cholectrack20
RESULTS_ROOT=./results

# Training Configuration
BATCH_SIZE=8
LEARNING_RATE=0.001
NUM_EPOCHS=50
DEVICE=cpu

# Logging Configuration
LOG_LEVEL=INFO
SAVE_CHECKPOINTS=true
CHECKPOINT_FREQ=10
